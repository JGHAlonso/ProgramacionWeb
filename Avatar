<script>


var c = document.getElementById("myCanvas");
var ctx = c.getContext("2d");


ctx.beginPath();
       ctx.arc(250, 160, 50, 0, 2 * Math.PI);
       ctx.strokeStyle = 'black';
       ctx.fillStyle = 'black';

       ctx.fill();
       ctx.stroke();
       ctx.fillStyle="rgb(200,0,0)";
ctx.fillStyle="	#F7DCC7";
ctx.fillRect(200,150,100,100);

ctx.fillStyle="	black";
ctx.fillRect(200,150,150,20);

ctx.fillStyle="	white";
